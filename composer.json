{
	"name": "thomasboom89/bundesliga-live-result-scraper",
	"description": "This is a scraper for Deutsche Fussball Bundesliga 1 & 2 live results ",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/ThomasBoom89/bundesliga-live-result-scraper/issues"
	},
	"authors": [
		{
			"name": "ThomasBoom89",
			"email": "51998416+ThomasBoom89@users.noreply.github.com",
			"homepage": "https://github.com/ThomasBoom89/bundesliga-live-result-scraper",
			"role": "owner"
		}
	],
	"autoload": {
		"psr-4": {
			"ThomasBoom89\\BundesligaLiveResults\\": "src/"
		}
	},
	"require": {
		"ext-dom": "*",
		"php": ">=8.1",
		"psr/http-client": "^1.0",
		"psr/http-factory": "^1.0"
	},
	"require-dev": {
		"captainhook/captainhook": "5.18.*",
		"guzzlehttp/guzzle": "7.*",
		"php-http/mock-client": "1.6.*",
		"phpmd/phpmd": "2.14.*",
    "phpstan/phpstan": "1.10.*",
		"phpunit/phpunit": "10.5.*",
		"roave/security-advisories": "dev-latest",
		"squizlabs/php_codesniffer": "3.7.*",
		"thomasboom89/captainhook-collection": "^0.0.4",
		"twig/twig": "^3.5"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	}
}
