{
	"name": "thomasboom89/bundesliga-live-results",
	"autoload": {
		"psr-4": {
			"ThomasBoom89\\BundesligaLiveResults\\": "src/"
		}
	},
	"require": {
		"ext-dom": "*",
		"php": ">=8.1",
		"psr/http-client": "^1.0",
		"psr/http-factory": "^1.0"
	},
	"require-dev": {
		"captainhook/captainhook": "5.14.*",
		"guzzlehttp/guzzle": "7.*",
		"php-http/mock-client": "1.5.*",
		"phpmd/phpmd": "2.13.*",
		"phpstan/phpstan": "1.9.*",
		"phpunit/phpunit": "9.5.*",
		"roave/security-advisories": "dev-latest",
		"squizlabs/php_codesniffer": "3.7.*",
		"thomasboom89/captainhook-collection": "^0.0.3",
		"twig/twig": "^3.5"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	}
}
